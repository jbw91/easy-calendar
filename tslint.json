{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "trailing-comma": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "member-access": false,
      "whitespace": [
        true,
        "check-decl",
        "check-operator",
        "check-module",
        "check-separator",
        "check-rest-spread",
        "check-type",
        "check-typecast",
        "check-type-operator",
        "check-preblock"
      ],
      "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
      ],
      "no-empty": false
    },
    "rulesDirectory": []
}
